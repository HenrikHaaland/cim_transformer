<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:cim="http://iec.ch/TC57/2013/CIM-schema-cim16#">

 <cim:PowerTransformer rdf:ID="urn:uuid:4a2e6276-9e74-4d6e-8583-f3cfda35b9c7">
    <cim:name>Main Transformer</cim:name>
 </cim:PowerTransformer>

  <!-- Substation -->
  <cim:Substation rdf:ID="Substation1">
    <cim:IdentifiedObject.name>Substation 1</cim:IdentifiedObject.name>
  </cim:Substation>

  <!-- Voltage Level -->
  <cim:VoltageLevel rdf:ID="VoltageLevel1">
